<Window x:Class="SpotifyInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpotifyInterface"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800"
         Background="#19FF8A">

    <Grid>
        <Label x:Name="UserEntrat"
           FontSize="18"
           HorizontalAlignment="Left"
           VerticalAlignment="Top"
           Margin="20,20,0,0" />
        
        <Button Content="Songs" Click="OpenSongsWindow" HorizontalAlignment="Left" Height="90" Margin="108,202,0,0" VerticalAlignment="Top" Width="180"/>
        
    </Grid>

</Window>